{
    "java.project.sourcePaths": [
        "src"
    ],
    "java.project.outputPath": "bin",
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "cSpell.words": [
        "defaultmethods",
        "eazybytes"
    ],
    "code-runner.executorMap": {
    "java": "cd $workspaceRoot && javac -d bin -cp $workspaceRoot/bin:$workspaceRoot/lib/* src/$relativeFile && java -cp $workspaceRoot/bin:$workspaceRoot/lib/* ${relativeFileDir//\\//.}.$fileNameWithoutExt"
  }
}